{
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "always",
    "overrides": [
        {
            "files": [
                "*.html"
            ],
            "options": {
                "parser": "html"
            }
        },
        {
            "files": [
                "*.md"
            ],
            "options": {
                "parser": "markdown"
            }
        },
        {
            "files": [
                "*.yaml",
                "*.yml"
            ],
            "options": {
                "parser": "yaml"
            }
        },
        {
            "files": [
                "*.toml"
            ],
            "options": {
                "parser": "toml"
            }
        }
    ],
    "plugins": [
        "prettier-plugin-toml"
    ]
}